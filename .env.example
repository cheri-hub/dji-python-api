# ==============================================================
# DJI AG API - Environment Variables
# ==============================================================

# DJI Account Credentials (obrigatório)
DJI_USERNAME=seu_email@exemplo.com
DJI_PASSWORD=sua_senha

# API Security (obrigatório)
# Gere uma chave segura: openssl rand -hex 32
API_KEY=sua_chave_secreta_aqui

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/api

# Browser Configuration
BROWSER_HEADLESS=true
BROWSER_PROFILE_DIR=./browser_profile

# Download Configuration
DOWNLOADS_DIR=./downloads

# Coordinate Filters (Brazil - opcional)
LAT_MIN=-35
LAT_MAX=-5
LON_MIN=-75
LON_MAX=-35
